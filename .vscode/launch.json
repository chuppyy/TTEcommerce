{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "TTEcommerce.Api",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "BuildWebApi",
            "program": "${workspaceFolder}/TTEcommerce.Api/bin/Debug/netcoreapp3.1/TTEcommerce.Api.dll",
            "args": [],
            "cwd": "${workspaceFolder}/TTEcommerce.Api",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            },
            "dotnetRunMessages": true
        },
        {
            "name": "TTEcommerce.Blazor",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "BuildBlazorApp",
            "program": "${workspaceFolder}/TTEcommerce.Blazor/bin/Debug/netcoreapp3.1/TTEcommerce.Blazor.dll",
            "args": [],
            "cwd": "${workspaceFolder}/TTEcommerce.Blazor",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            },
            "dotnetRunMessages": true
        }
    ]
}
